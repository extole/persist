<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <!--<logger name="persist" level="DEBUG"/>-->
    <!--<logger name="persist.engine" level="DEBUG"/>-->
    <!--<logger name="persist.parameters" level="DEBUG"/>-->
    <!--<logger name="persist.results" level="DEBUG"/>-->
    <!--<logger name="persist.profiling" level="DEBUG"/>-->

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>